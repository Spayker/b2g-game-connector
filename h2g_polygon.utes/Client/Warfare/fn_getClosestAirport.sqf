/*	Return the closest airport of the given entitie.	 Parameters:		- Object*/Private ["_closest","_near","_pos","_range"];_closest = objNull;_pos = _this select 0;_range = _this select 1;_near = [_pos, _range] Call WFCL_FNC_GetClosestDepot;if !(isNull (_near)) then {    _townSpecialities = _near getVariable "townSpeciality";    if (WF_C_AIR_BASE in _townSpecialities) then { _closest = _near }};_closest